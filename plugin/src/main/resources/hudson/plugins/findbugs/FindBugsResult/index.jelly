<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler"
	xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson"
	xmlns:f="/lib/form" xmlns:i="jelly:fmt" xmlns:result="/result">
	<l:layout norefresh="true">
		<st:include it="${it.owner}" page="sidepanel.jelly" />
		<l:main-panel>

			<h1>${%FindBugs Result}</h1>

			<h2>${%Warnings Trend}</h2>

			<table class="pane" id="summary">
				<tr>
					<td class="pane-header">${%All Warnings}</td>
					<td class="pane-header">${%New this build}</td>
                    <j:if test="${it.newThisWeek != 0}">
                        <td class="pane-header">
                            <img src="/plugin/findbugs/icons/fb-cloud-icon-small.png" title="FindBugs Cloud"/>
                            ${%New this week}</td>
                    </j:if>
                    <j:if test="${it.notInCloud != 0}">
                        <td class="pane-header">
                            <img src="/plugin/findbugs/icons/fb-cloud-icon-small.png" title="FindBugs Cloud"/>
                            ${%Not uploaded}</td>
                    </j:if>
					<td class="pane-header">${%Fixed Warnings}</td>
				</tr>
				<tbody>
					<tr>
						<td class="pane">
							${it.numberOfAnnotations}
						</td>
						<td class="pane">
							<j:if test="${it.numberOfNewWarnings == 0}">
								0
							</j:if>
							<j:if test="${it.numberOfNewWarnings != 0}">
								<a href="new">${it.numberOfNewWarnings}</a>
							</j:if>
						</td>
                        <j:if test="${it.newThisWeek != 0}">
						    <td class="pane">
								${it.newThisWeek}
						    </td>
                        </j:if>
                        <j:if test="${it.notInCloud != 0}">
						    <td class="pane">
								${it.notInCloud}
						    </td>
                        </j:if>
						<td class="pane">
							<j:if test="${it.numberOfFixedWarnings == 0}">
								0
							</j:if>
							<j:if test="${it.numberOfFixedWarnings != 0}">
								<a href="fixed">${it.numberOfFixedWarnings}</a>
							</j:if>
						</td>
					</tr>
				</tbody>
			</table>

            <result:priorities-summary />

	        <result:main />

		</l:main-panel>
	</l:layout>
</j:jelly>
